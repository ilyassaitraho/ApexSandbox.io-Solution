public String whichTwo(Integer a, Integer b, Integer c) {
    if (a + b == c) {
        return 'c';  // c is the sum of a and b
    } else if (a + c == b) {
        return 'b';  // b is the sum of a and c
    } else if (b + c == a) {
        return 'a';  // a is the sum of b and c
    }
    return '';  // No two add up to the third
