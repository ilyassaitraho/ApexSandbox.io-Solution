public Integer fibonacci(Integer n) {
   
    if (n == 1 || n == 2) {
        return 1;
    }
    
    Integer prev = 1;
    Integer current = 1;
    
    for (Integer i = 3; i <= n; i++) {
        Integer next = prev + current;
        prev = current;
        current = next;
    }
    
    return current;
}