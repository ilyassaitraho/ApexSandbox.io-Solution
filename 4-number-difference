public Integer diff(Integer a, Integer b) {
  if ( a != null && b != null &&  a > b ) {
      Integer diff = a-b ; 
      return diff ; 
  } else {
      return b-a ;
  }
}
/*
or 
public Integer diff(Integer a, Integer b) {
    if (a != null && b != null) {
        return Math.abs(a - b);
    }
    return null;
} */